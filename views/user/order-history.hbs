<section>
    <div class="container" style="padding-left:200px;">
        {{!-- <div class="title mb-5">
            <p>Hai, &#128075; {{user.username}}</p>
        </div> --}}

        {{#each response}}
        <div class="card my-4">
            <h5 class="card-header" style="background-color:grey; color:white; font-size: 15px;">
                {{this.statusUpdateDate}}
            </h5>
            <div class="d-flex card-font-size">
                <img class="img-fluid ms-2 my-2"
                    src="{{this.products.image}}" alt="Card image cap"
                    width="120">
                <div class="card-body ms-5 w-25">
                    <h5 class="card-title">{{this.products.proName}}</h5>
                    <p class="card-text">Quantity: {{this.products.quantity}}</p>
                    <p class="card-text">Amount: â‚¹{{this.products.price}}</p>
                </div>
                <div class="card-body w-25">
                    <p class="card-text">{{this.deliveryDetails.fullname}}, {{this.deliveryDetails.address}}</p>
                    <p class="card-text">{{this.deliveryDetails.landmark}}, {{this.deliveryDetails.state}}</p>
                    <p class="card-text">{{this.deliveryDetails.pincode}}, {{this.deliveryDetails.country}}</p>
                </div>
                <div class="card-body">
                    <p class="card-text">Payment Method: {{this.paymentMethod}}</p>
                    <span class="card-text" style="color: blue;">Order Status:
                        <span id="{{this._id}}{{this.products.proId}}" style="color: {{#ifCheck products.status 'canceled'}}
                    red
                    {{else}}
                    green
                    {{/ifCheck}}">{{this.products.status}}</span>
                    </span>
                    <br>
                    <br>
                    {{#ifCheck products.status 'delivered'}}
                    {{#ifDateCheck this.statusUpdateDate }}
                    <button id="{{this.products.proId}}{{this._id}}"
                        onclick="changeOrderStatus('{{this._id}}','{{this.products.proId}}','returned')"
                        class="btn btn-success">Return Product</button>
                    {{/ifDateCheck}}
                    {{/ifCheck}}
                    {{#ifCheck products.status 'placed'}}
                    <button id="{{this.products.proId}}{{this._id}}"
                        onclick="changeOrderStatus('{{this._id}}','{{this.products.proId}}','canceled')"
                        class="btn btn-danger">Cancel
                        Order</button>
                    {{/ifCheck}}
                    {{#ifCheck products.status 'shipped'}}
                    <button id="{{this.products.proId}}{{this._id}}"
                        onclick="changeOrderStatus('{{this._id}}','{{this.products.proId}}','canceled')"
                        class="btn btn-danger">Cancel
                        Order</button>
                    {{/ifCheck}}
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</section>